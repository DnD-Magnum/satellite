{
    "name": "php-etl/satellite",
    "description": "This library aims at building and running lambda PHP functions",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiboko SAS",
            "homepage": "http://kiboko.fr"
        },
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "psr/log": "^1.1",
        "symfony/console": "^5.2",
        "symfony/process": "^5.2",
        "symfony/messenger": "^5.2",
        "symfony/config": "^5.2",
        "monolog/monolog": "^2.0@dev",
        "ramsey/uuid": "^4.0@dev",
        "php-etl/fast-map-plugin": "^0.1",
        "php-etl/csv-plugin": "^0.1",
        "php-etl/akeneo-plugin": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "johnkary/phpunit-speedtrap": "*",
        "mybuilder/phpunit-accelerator": "*",
        "phpunit/php-invoker": "*",
        "fzaninotto/faker": "^1.9@dev",
        "phpspec/phpspec": "^7.0",
        "friends-of-phpspec/phpspec-code-coverage": "^6.0",
        "php-etl/pipeline": "0.2.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\Satellite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Kiboko\\Component\\Satellite\\": "spec/",
            "unit\\Kiboko\\Component\\Satellite\\": "unit/",
            "functional\\Kiboko\\Component\\Satellite\\": "functional/",
            "performance\\Kiboko\\Component\\Satellite\\": "performance/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}

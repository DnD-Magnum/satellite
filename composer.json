{
    "name": "php-etl/satellite",
    "description": "This library aims at building and running lambda PHP functions",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiboko SAS",
            "homepage": "http://kiboko.fr"
        },
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "symfony/console": "^4.4 || ^5.0",
        "symfony/process": "^4.4",
        "psr/log": "^1.1@dev",
        "php-etl/promise": "^1.0@dev",
        "ramsey/uuid": "^4.0@dev",
        "monolog/monolog": "^2.0@dev",
        "symfony/messenger": "^5.2@dev",
        "php-etl/fast-map": "@dev",
        "php-etl/pipeline": "@dev",
        "php-etl/fast-map-config": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "10.0.x-dev",
        "phpspec/phpspec": "8.0.x-dev",
        "fzaninotto/faker": "1.9.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\Satellite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Kiboko\\Component\\Satellite\\": "tests/spec/",
            "unit\\Kiboko\\Component\\Satellite\\": "tests/unit/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
